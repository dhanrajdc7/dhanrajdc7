#include<bits/stdc++.h>
using namespace std;
int prime( long long int x)
{
	if(x==1)
	{
		return false;
	}
	if(x==2)
	{
		return true;
	}
	for( long long int i=2; i*i<=x; i++)
	{
		if(x%i==0)
		{
			return false;
		}
	}
	return true;
 }
 int main()
 { int t;
 cin>>t;
 while(t--)
 {
 
 long long	int n;
 	cin>>n;
 long long	int x=prime(n);
 	if(x==1)
 	{
 		cout<<"YES"<<endl;
	 }else
	 {
	 	cout<<"NO"<<endl;
	 }
}
 }
  
